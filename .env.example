# Wallet Configuration
SOLANA_PRIVATE_KEYS=key1,key2,key3
ETH_PRIVATE_KEYS=key1,key2,key3
ENCRYPTION_KEY=your_encryption_key_here

# RPC Endpoints
SOLANA_RPC_MAINNET=https://api.mainnet-beta.solana.com
SOLANA_RPC_TESTNET=https://api.testnet.solana.com
SOLANA_RPC_DEVNET=https://api.devnet.solana.com
ETH_RPC_SEPOLIA=https://rpc.sepolia.org

# Proxy Configuration
PROXY_LIST=proxy1:port:user:pass,proxy2:port:user:pass

# Captcha Services
TWOCAPTCHA_API_KEY=your_api_key
ANTICAPTCHA_API_KEY=your_api_key

# Database
DATABASE_PATH=data/bot.db

# Bot Settings
MIN_DELAY_SECONDS=30
MAX_DELAY_SECONDS=120
DAILY_TRANSACTIONS_PER_WALLET=10
TESTNET_MODE=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/bot.log